    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="54.0" prefWidth="170.0" text="Welcom to RideBuddies">
      <font>
         <Font size="15.0" />
      </font>
   </Label>
   <Button mnemonicParsing="false" onAction="#switchToRegister" prefHeight="26.0" prefWidth="214.0" text="Register" />
   <Button mnemonicParsing="false" onAction="#switchToLogin" prefHeight="26.0" prefWidth="222.0" text="Login" />

    <Label fx:id="welcomeText" />
</VBox>
